<h1> Your card sets:</h1>
<% if @card_sets.any? %>
  <% @card_sets.each do |card_set| %>
  <p>
    <%= card_set.set_name %> =>
    <%= link_to "Edit", edit_card_set_path(card_set.id) %> |
    <%= link_to "Go Flashcards", cards_path(@display_keys) %>
  </p>
  <% end %>
<% else %>
  <p>You do not currently have any sets.</p>
<% end %>
<br>

<%= link_to "Create new card set", new_card_set_path %>

