<div style= "margin-top: 40px;">
  <h1> Create a new card set</h1>
  <%= form_tag '/card_sets', :method => 'post' do %>
  <%= render "layouts/card_sets_form"%>
    <%= label_tag(:set_name, "Set Name:") %>
    <%= text_field_tag(:set_name) %>
    <hr>
    <%= submit_tag "Save Set", class: "btn" %>
    <% end %>
    </div>
  </div>
</div>

